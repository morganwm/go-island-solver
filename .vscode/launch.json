    "version": "0.2.0",
    "configurations": [
        {
            "name": "Tests",
            "type": "go",
            "request": "launch",
            "mode": "test",
            "program": "${workspaceFolder}/core",
            "args": [
                "-test.v",
            ]
        },
        {
            "name": "Test - Amazon Rectangular",
            "type": "go",
            "request": "launch",
            "mode": "test",
            "program": "${workspaceFolder}/core",
            "args": [
                "-test.v",
                "-test.run=^TestIslandCounter/amzn_.*$"
            ]
        },
        {
            "name": "Benchmarks",
            "type": "go",
            "request": "launch",
            "mode": "test",
            "program": "${workspaceFolder}/core",
            "args": [
                "-test.benchmem",
                "-test.bench=^(Benchmark).*$",
            ]
        },
        {
            "name": "Run With UI",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "console": "integratedTerminal",
            "program": "${workspaceFolder}",
        },
        {
            "name": "Run Parallel With UI",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "console": "integratedTerminal",
            "program": "${workspaceFolder}",
            "args": ["-mode=dfs/parallel"],
        },
        {
            "name": "Run",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": ["-basic-output"]
        },
        {
            "name": "Run Parallel",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": ["-basic-output", "-mode=dfs/parallel"]
        },
        {
            "name": "Version",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": ["-version"]
        },
        {
            "name": "Help",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": ["-help"]
        },
    ]
}